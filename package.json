{
  "name": "mark2",
  "version": "0.0.0-development",
  "description": "A collective of sequencers for scheduling events",
  "main": "src/index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "commit": "git-cz",
    "test": "mocha ./src --compilers js:babel-register --require ./test/helpers.js --require ./test/dom.js --recursive -w",
    "test:single": "istanbul cover -x *.spec.js _mocha -- -R spec ./src",
    "tdd": "npm test -- --watch",
    "dev": "webpack-dev-server --port 3000 --devtool eval --progress --colors --hot --content-base dist",
    "build": "webpack -p",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "check-converage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/minnam/mark2.git"
  },
  "keywords": [
    "sequencer"
  ],
  "author": "Min Nam <hi@minnam.io> (http://minnam.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minnam/mark2/issues"
  },
  "homepage": "https://github.com/minnam/mark2#readme",
  "devDependencies": {
    "babel": "6.23.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "enzyme": "3.1.0",
    "eslint": "4.10.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "ghooks": "2.0.0",
    "istanbul": "0.4.5",
    "jsdom": "11.3.0",
    "mocha": "4.0.1",
    "react-addons-test-utils": "15.6.2",
    "semantic-release": "^8.2.0",
    "sinon": "4.0.2",
    "webpack": "3.8.1"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "pre-commit": "npm run test:single npm run check-coverage"
  }
}
